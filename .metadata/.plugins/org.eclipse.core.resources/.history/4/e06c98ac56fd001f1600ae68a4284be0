<script type="text/javascript">
	
	// 페이지 요청 시 호출
	function initialize()
	{
		container = document.getElementById("map");
		options = 
		{
			center: new kakao.maps.LatLng(37.556529, 126.919510);
			, level: 3
		}
		
		map = new kakao.maps.Map(container, options);
	}

</script>


</head>
<body onload="initialize()">

